<?php

/*
* O PHP só consegue trabalhar com array associativo que sejam variáveis primitivas do tipo String e Integer
* Se quiser saber mais: https://www.php.net/manual/pt_BR/language.types.array.php
*/ 

$array = [
    1 => 'a',
    '1' => 'b',
    '1.5' => 'c',
    true => 'd',
];

foreach ($array as $item) {
    echo $item . PHP_EOL;
}